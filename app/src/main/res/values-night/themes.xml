<resources xmlns:tools="http://schemas.android.com/tools">

    <style
        name="AppStaticTheme"
        parent="Theme.Material3.Dark.NoActionBar">
        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">?attr/colorSurface</item>
        <item
            name="android:forceDarkAllowed"
            tools:targetApi="q">false</item>
    </style>

    <style
        name="AppTheme"
        parent="Theme.Material3.Dark.NoActionBar">
        <item name="colorPrimary">@color/theme_1_md_theme_dark_primary</item>
        <item name="colorOnPrimary">@color/theme_1_md_theme_dark_onPrimary</item>
        <item name="colorPrimaryContainer">@color/theme_1_md_theme_dark_primaryContainer</item>
        <item name="colorOnPrimaryContainer">@color/theme_1_md_theme_dark_onPrimaryContainer</item>
        <item name="colorSecondary">@color/theme_1_md_theme_dark_secondary</item>
        <item name="colorOnSecondary">@color/theme_1_md_theme_dark_onSecondary</item>
        <item name="colorSecondaryContainer">@color/theme_1_md_theme_dark_secondaryContainer</item>
        <item name="colorOnSecondaryContainer">@color/theme_1_md_theme_dark_onSecondaryContainer</item>
        <item name="colorTertiary">@color/theme_1_md_theme_dark_tertiary</item>
        <item name="colorOnTertiary">@color/theme_1_md_theme_dark_onTertiary</item>
        <item name="colorTertiaryContainer">@color/theme_1_md_theme_dark_tertiaryContainer</item>
        <item name="colorOnTertiaryContainer">@color/theme_1_md_theme_dark_onTertiaryContainer</item>
        <item name="colorError">@color/theme_1_md_theme_dark_error</item>
        <item name="colorErrorContainer">@color/theme_1_md_theme_dark_errorContainer</item>
        <item name="colorOnError">@color/theme_1_md_theme_dark_onError</item>
        <item name="colorOnErrorContainer">@color/theme_1_md_theme_dark_onErrorContainer</item>
        <item name="android:colorBackground">@color/theme_1_md_theme_dark_background</item>
        <item name="colorOnBackground">@color/theme_1_md_theme_dark_onBackground</item>
        <item name="colorSurface">@color/theme_1_md_theme_dark_surface</item>
        <item name="colorOnSurface">@color/theme_1_md_theme_dark_onSurface</item>
        <item name="colorSurfaceVariant">@color/theme_1_md_theme_dark_surfaceVariant</item>
        <item name="colorOnSurfaceVariant">@color/theme_1_md_theme_dark_onSurfaceVariant</item>
        <item name="colorOutline">@color/theme_1_md_theme_dark_outline</item>
        <item name="colorOnSurfaceInverse">@color/theme_1_md_theme_dark_inverseOnSurface</item>
        <item name="colorSurfaceInverse">@color/theme_1_md_theme_dark_inverseSurface</item>
        <item name="colorPrimaryInverse">@color/theme_1_md_theme_dark_inversePrimary</item>

        <item name="android:windowLightStatusBar">true</item>
        <item name="android:statusBarColor">@color/theme_1_md_theme_dark_surface</item>
        <item
            name="android:forceDarkAllowed"
            tools:targetApi="q">false</item>

        <item name="android:windowSplashScreenBackground">@color/theme_1_md_theme_dark_surface</item>
        <item name="android:windowSplashScreenAnimatedIcon">@drawable/ic_launcher_background</item>
        <item name="android:windowSplashScreenAnimationDuration">1000</item>
        <item name="android:windowSplashScreenIconBackgroundColor">@color/theme_1_md_theme_dark_surface</item>
        <item name="android:windowSplashScreenBehavior">icon_preferred</item>
    </style>

